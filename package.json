{
  "name": "n8n-nodes-pdf4me-excel",
  "version": "1.0.1",
  "description": "n8n community node for Excel document processing with PDF4me API - Add headers and footers to Excel worksheets",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "excel",
    "spreadsheet",
    "headers",
    "footers",
    "pdf4me",
    "office",
    "documents",
    "automation"
  ],
  "homepage": "https://dev.pdf4me.com",
  "bugs": {
    "url": "https://github.com/pdf4me/n8n-nodes-pdf4me-excel/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdf4me/n8n-nodes-pdf4me-excel.git"
  },
  "license": "MIT",
  "author": "PDF4me <support@pdf4me.com>",
  "type": "commonjs",
  "main": "index.js",
  "files": [
    "dist",
    "index.js",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && gulp build",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run validate-build && eslint -c .eslintrc.prepublish.js nodes credentials package.json",
    "validate-build": "node -e \"const fs=require('fs'); const path=require('path'); const requiredFiles=['dist/nodes/Pdf4me/Pdf4me.node.js','dist/credentials/Pdf4meApi.credentials.js']; requiredFiles.forEach(f=>{if(!fs.existsSync(f))throw new Error('Missing required file: '+f);});\""
  },
  "devDependencies": {
    "@types/node": "20.19.7",
    "@typescript-eslint/eslint-plugin": "~5.45",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "merge-stream": "^2.0.0",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=20.15"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Pdf4meApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Pdf4me/Pdf4me.node.js"
    ]
  }
}
